{
    "name": "StackDoc",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Adds Stackoverflow question lists to MSDN documentation.",
    "content_scripts": [
        {
            "matches": ["http://msdn.microsoft.com/*"],
            "css": ["popover.css", "content.css"],
            "js": ["jquery-1.7.2.min.js", "jquery.popover-1.1.0.js", "Mustache.js", "content.js"]
        }
      ],
    "permissions": [
        "http://msdn.microsoft.com/*"
    ],
    "icons": {
        "16": "icons/icon-16.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    }
}
